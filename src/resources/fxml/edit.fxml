         AnchorPane.topAnchor="0.0">
      <TextField fx:id="nameField" layoutX="86.0" layoutY="22.0" promptText="name"/>
      <TextField fx:id="ipField" layoutX="86.0" layoutY="55.0" promptText="8.8.8.8"/>
      <Button defaultButton="true" layoutX="140.0" layoutY="91.0" mnemonicParsing="false" onAction="#btnOk"
              text="ok"/>
      <Button cancelButton="true" layoutX="186.0" layoutY="91.0" mnemonicParsing="false" onAction="#btnCancel"
              text="cancel"/>
      <Label layoutX="39.0" layoutY="26.0" text="name" textAlignment="RIGHT"/>
      <Label layoutX="58.0" layoutY="59.0" text="ip" textAlignment="RIGHT"/>
      <Label fx:id="labelMessage" layoutY="99.0" textFill="#12b509">
         <padding>
            <Insets left="10.0"/>
         </padding>
         <font>
            <Font size="14.0"/>
         </font>
      </Label>
   </Pane>
</AnchorPane>
