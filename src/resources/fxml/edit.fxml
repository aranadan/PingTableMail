<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="120.0" prefWidth="250.0"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="pingTableMail.controllers.DialogController">
   <Pane layoutX="-121.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="150.0" prefWidth="300.0"
